<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.scene.LoadingSceneController">
    <Label fx:id="welcomeText" />
    <Button fx:id="loginButton" layoutX="485.0" layoutY="50.0" onAction="#login" prefHeight="70.0" prefWidth="107.0" text="Play">
      <font>
         <Font size="24.0" />
      </font></Button>
   <TextField fx:id="nameTextField" layoutX="485.0" layoutY="25.0" prefHeight="16.0" prefWidth="107.0" />
   <Text layoutX="485.0" layoutY="21.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Type your name here:" />
   <GridPane layoutX="200.0" layoutY="155.0">
     <columnConstraints>
       <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
       <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
     </columnConstraints>
     <rowConstraints>
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
     </rowConstraints>
      <children>
         <Button mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" text="Button" />
         <Button mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" text="Button" GridPane.columnIndex="1" />
         <Button mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" text="Button" GridPane.rowIndex="1" />
         <Button mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" text="Button" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         <Button mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" text="Button" GridPane.rowIndex="2" />
         <Button mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" text="Button" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      </children>
   </GridPane>
</AnchorPane>
